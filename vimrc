syntax on
filetype on
set nobackup
set nocompatible
set modelines=0
set number
set encoding=utf-8
set nowrap
set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set copyindent
set expandtab
set noshiftround
set hlsearch
set incsearch
set showmatch
set smartcase
set hidden
set showcmd
set background=dark
set laststatus=2

inoremap jj <ESC>
